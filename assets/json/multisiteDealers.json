{
		"amazon": {
			"similar": "^www\\.amazon\\.",
			"pattern": "^(https?:\\/\\/www\\.amazon\\.[a-z.]+)(((?!\\/dp\\/).)*)(\\/dp\\/|\\/gp\\/product\\/)([0-9A-Za-z]+).*$",
			"replacement": "$1$4$5"
		},
		"bergzeit": {
			"similar": "^www\\.bergzeit\\.",
			"pattern": "^(https?:\\/\\/www\\.bergzeit\\.[a-z.]+\\/)(.+)$",
			"replacement": "$1$2"
		},
		"bikester": {
			"similar": "^www\\.(bikester\\.|fahrrad.de)",
			"pattern": "^(https?:\\/\\/www\\.(bikester\\.[a-z.]+|fahrrad.de)\\/)(((?![0-9]+\\.html).)+)([0-9]+)\\.html$",
			"replacement": "$1$5"
		},
		"campz": {
			"similar": "^www\\.campz\\.",
			"pattern": "^(https?:\\/\\/www\\.campz\\.[a-z.]+\\/)(((?![0-9]+\\.html).)+)([0-9]+)\\.html$",
			"replacement": "$1$4"
		},
		"conrad": {
			"similar": "^www\\.(conrad\\.|conrad-electronic\\.co\\.uk)",
			"pattern": "^(https?:\\/\\/www\\.(conrad\\.[a-z.]+|conrad-electronic\\.co\\.uk)\\/ce\\/)[a-z]{2}\\/product\\/([0-9]+)\\/.*$",
			"replacement": "$1Search.html?search=$3"
		},
		"decathlon": {
			"similar": "^www\\.decathlon\\.",
			"pattern": "^(https?:\\/\\/www\\.decathlon\\.[a-z.]+\\/)(.+)$",
			"replacement": "$1$2"
		},
		"zalando": {
			"similar": "^www\\.zalando\\.",
			"pattern": "^(https?:\\/\\/)[a-z]{2,3}(\\.zalando\\.[a-z.]+\\/)(((?![a-z0-9-]{13}\\.html).)+)([a-z0-9-]{13})\\.html$",
			"replacement": "$1www$2?q=$5"
		}
}